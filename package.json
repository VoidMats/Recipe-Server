  "author": "Mats Eriksson <mats.eriksson.ingemar@gmail.com>",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "start": "parcel",
    "build": "rm -rf .parcel-cache && parcel build src/index.html --detailed-report --reporter @parcel/reporter-bundle-analyzer",
