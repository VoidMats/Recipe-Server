    "mongodb": "^6.6.2",
    "mongodb-uri": "^0.9.7"
  },
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.12.0",
    "crypto-browserify": "^3.12.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.12.0",
    "path-browserify": "^1.0.0",
    "stream-browserify": "^3.0.0",
    "vm-browserify": "^1.1.2"
  }
